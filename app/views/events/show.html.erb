<h1>Events#show</h1>
<p>Event: <%= @event.title%>, Location: <%= @event.location %>, Date: <%= @event.date %>,Creator: <%= @event.creator.name%></p>
<h3>Atendees</h3>
<% if @event.attendees %>
	<ul>
	<% @event.attendees.each do |attendee| %>
		<li><%= attendee.name %></li>
	<% end %>
	</ul>
<% end %>	

